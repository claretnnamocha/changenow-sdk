{
  "env": {
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-param-reassign": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "camelcase": 0
  }
}
